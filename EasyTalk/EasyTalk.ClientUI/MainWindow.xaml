<Window x:Class="EasyTalk.ClientUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyTalk.ClientUI"
        mc:Ignorable="d"
        Title="EasyTalk" MinHeight="400" MinWidth="800" Height="600" Width="1000" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <RadialGradientBrush>
            <GradientStop Color="#FF666666" Offset="1"/>
            <GradientStop Color="#FFAEAEAE"/>
            <GradientStop Color="#FF999999" Offset="0.374"/>
            <GradientStop Color="#FF818181" Offset="0.691"/>
        </RadialGradientBrush>
    </Window.Background>

    <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden"/>

</Window>
